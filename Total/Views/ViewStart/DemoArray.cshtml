@model List<Total.Models.Product>

@{
    ViewBag.Title = "DemoArray";
}

<!DOCTYPE html>

@if (Model.Count > 0)
{
    <table>
        <tr><td>Product</td><td>Price</td></tr>
        @foreach (var product in Model)
        {
            <tr><td>@product.Name</td><td>@product.Price</td></tr>
        }
    </table>
}
else
{
    @:No data
}